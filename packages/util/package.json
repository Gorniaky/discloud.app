{
  "name": "@discloudapp/util",
  "version": "0.4.7",
  "description": "A utility pack for discloud.app",
  "main": "out",
  "types": "out/index.d.ts",
  "scripts": {
    "watch": "tsc -watch",
    "build:release": "tsc",
    "prepublish": "npm run build:release",
    "release": "npm run prepublish && npm publish",
    "test": "tsc && npm run test:ts && npm run test:unit",
    "test:ts": "tsc --noEmit",
    "test:unit": "node out/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/discloud/discloud.app.git"
  },
  "keywords": [
    "discloud",
    "discloud.app"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "glob": "^10.3.4",
    "undici": "^5.23.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
