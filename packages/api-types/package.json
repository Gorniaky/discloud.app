{
  "name": "@discloudapp/api-types",
  "version": "0.1.1",
  "scripts": {
    "build:release": "tsc",
    "clean:node": "rimraf ./{rest,v*}/**/*.{js,mjs,d.ts,*map} ./v*.{js,mjs,d.ts,*map}",
    "postpublish": "npm run clean:node",
    "prepublish": "npm run build:release",
    "release": "npm run prepublish && npm publish",
    "test:ts": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gorniaky/discloud.app.git"
  },
  "keywords": [
    "discloud"
  ],
  "author": "Gorniaky <hwss.s2@gmail.com>",
  "license": "Apache 2.0",
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3"
  },
  "exports": {
    "./v2": {
      "require": "./v2.js",
      "import": "./v2.js",
      "types": "./v2.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
